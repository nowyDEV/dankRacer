{
  "name": "typing_game",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^5.2.4",
    "@emotion/babel-preset-css-prop": "^10.0.14",
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "@types/jest": "24.0.19",
    "@types/node": "12.11.6",
    "@types/react": "16.9.9",
    "@types/react-dom": "16.9.2",
    "axios": "^0.19.0",
    "mobx": "^5.11.0",
    "mobx-react": "^6.1.1",
    "phaser": "^3.20.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "3.2.0",
    "typescript": "3.6.4"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build && rm -rf dist && mv build dist",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint": "eslint \"**/*.+(ts|tsx|js)\"",
    "lint:css": "stylelint \"**/*.+(ts|js|css|scss|)\"",
    "format": "yarn prettier -- --write",
    "prettier": "prettier \"**/*.+(ts|tsx|js|jsx|json|css|scss|md|mdx)\"",
    "validate": "yarn lint && yarn prettier -- --list-different"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn check --integrity"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-cypress": "^2.7.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^2.0.1",
    "husky": "^4.0.0-beta.5",
    "lint-staged": "^9.2.5",
    "prettier": "^1.17.1",
    "prettier-eslint": "^9.0.0",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-custom-processor-loader": "^0.6.0",
    "stylelint-processor-styled-components": "^1.8.0"
  }
}
